# Woofalytics Environment Variables
# ---------------------------------
# Copy this file to .env and fill in your values.
# These take precedence over config.yaml settings.

# IFTTT Webhook Key (for notifications)
# Get yours at: https://ifttt.com/maker_webhooks
IFTTT_KEY=

# Log level override
LOG_LEVEL=INFO

# Timezone (for Docker)
TZ=Europe/London

# Optional: Override specific settings
# WOOFALYTICS__AUDIO__DEVICE_NAME=ReSpeaker
# WOOFALYTICS__MODEL__THRESHOLD=0.90
# WOOFALYTICS__DOA__ENABLED=true
